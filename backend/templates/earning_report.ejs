			<h1 class="header_title">Book My Dine</h1>
		</div>
		<div class="info">
			<div class="left">
				<div class="info_name">
					<b><%= res_name %> </b>
				</div>
				<div class="info_address"><%= res_address %></div>
				<div class="info_address"><%= res_phone %></div>
			</div>
			<div class="right">
				<div class="info_name">FROM <%= from_date %></div>
				<div class="info_name">TO <%= to_date %></div>
			</div>
		</div>
		<div class="title">Earning Summary Report</div>

		<table style="width: 100%; border-collapse: collapse">
			<thead>
				<tr class="">
					<th class="table_header_th">ID</th>
					<th class="table_header_th">Type</th>
					<th class="table_header_th">Earning</th>
					<th class="table_header_th">Date</th>
				</tr>
			</thead>
			<tbody>
				<% data.forEach(order => { %>
				<tr class="table_row">
					<td><%= order._id %></td>
					<td><%= order.type %></td>
					<td><%= order.earning %> â‚¹</td>
					<td><%= order.date %></td>
				</tr>
				<% }) %>
			</tbody>
		</table>
	</body>
</html>
